<!DOCTYPE html>
<html>
  <head>
    <% if content_for?(:title) %>
    <%= content_for(:title) %>
    <% else %>
      <title>Hi-Low Game</title>
    <% end %>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
